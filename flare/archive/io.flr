extern puts: (str) -> unit
extern printf: (str, ...) -> unit

struct IO[?T] =
    v: ?T

def IO[?T]
    in new: () -> IO[?T] = 
        return IO {v: 0}

    in new_with: (x: ?T) -> IO [?T] = 
        IO {v: x}

    in out: (self, s: str) -> IO[unit] =
        puts(s)
        IO {v: unit}

    in print_int: (self, v: int) -> IO[unit] = 
        printf("%d", v)
        IO {v: unit}

    in print_str: (self, v: str) -> IO[unit] = 
        printf("%s", v)
        IO {v: unit}



