package Option = 
    enum Option[?T] =
        Some { ?T },
        None

    # impl Option[?T] =
    #     def do_something self x y : ?T -> ?T -> ?T =
    #         match self
    #             | Some {_} then x,
    #             | None then y, 
    
    let unwrap_or o fallback : Option[?T] -> ?T -> ?T =
        match o
            | Some {n} then n,
            | None then fallback,

    let unwrap o : Option[?T] -> ?T =
        match o
            | Some {n} then n,
            | None then exit n,
    
    let is_some x : Option[?T] -> bool =
        match x
            | Some {_} then true,
            | None then false,

    
