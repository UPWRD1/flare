package Math =
    let add l r = l + r 
    let sub l r = l - r 
    let mul l r = l * r 
    let div l r = l / r 

    let factorial: num -> num x  =
        if x == 0 then
	        1
        else
           x + factorial (x - 1)
           
          # this is optional
          # add x (factorial (sub x 1))
